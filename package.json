{
  "name": "aristo",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "prebuild": "node scripts/link-assets.mjs && node scripts/generate-image-manifest.mjs && node scripts/generate-sitemap.mjs",
    "predev": "node scripts/link-assets.mjs && node scripts/generate-image-manifest.mjs && node scripts/generate-sitemap.mjs",
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "lint": "eslint src/",
    "format": "prettier --write src/",
    "format:check": "prettier --check src/",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "validate": "npm run type-check && npm run lint && npm run test",
    "add-entry": "node scripts/add-entry.mjs"
  },
  "dependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^7.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@fontsource/pt-sans": "^5.2.8",
    "@fontsource/pt-serif": "^5.2.8",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@vitejs/plugin-react": "^4.3.0",
    "@vitest/coverage-v8": "^4.0.18",
    "eslint": "^9.0.0",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.5.0",
    "exifr": "^7.1.3",
    "jsdom": "^28.0.0",
    "prettier": "^3.8.1",
    "sharp": "^0.34.5",
    "typescript": "~5.7.0",
    "typescript-eslint": "^8.55.0",
    "vite": "^6.1.0",
    "vitest": "^4.0.18"
  }
}
